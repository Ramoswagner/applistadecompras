<ItensSalvos>:
    MDFloatLayout:

        MDBoxLayout:
            orientation: "vertical"
            MDTopAppBar:
                title: "Voltar"
                md_bg_color: "#00DE73"  # Cor de fundo da barra superior
                title_color: "white"
                opposite_colors: True
                left_action_items: [["arrow-left", lambda x: app.dialogo_cancelar()]]
                right_action_items:
                    [["cart", lambda x: app.dialogo_ir_para_carrinho()]]
                
            MDScrollView:
                pos_hint: {"center_x": .5, "top": 0.80}
                size_hint: 1, 0.80
                MDList:
                    id: scroll_itens_salvos